    "clean": "rm -rf dist",
    "momo": "tsx src/index.ts"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "commander": "^14.0.3",
    "execa": "^9.6.1",
    "fs-extra": "^11.3.3",
    "gradient-string": "^3.0.0",
    "ora": "^9.1.0",
    "picocolors": "^1.1.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/gradient-string": "^1.1.6",
    "@types/node": "^25.2.0",
    "tsup": "^8.5.1",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "publishConfig": {
    "access": "public"
  }
}
